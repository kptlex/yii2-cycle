{
  "name": "kptlex/yii-cycle",
  "description": "Cycle ORM adapter for Yii2.",
  "keywords": [
    "cycle",
    "orm",
    "yii2",
    "yii"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Satyshev",
      "email": "kptlex@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lex\\Yii\\Cycle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lex\\Yii\\Cycle\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "cycle/annotated": "^2.0",
    "cycle/migrations": "^1.0",
    "cycle/orm": "^1.2",
    "yiisoft/yii2": "^2.0.41"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "3.*",
    "yiisoft/yii2-debug": "^2.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
